# Configuración del Servidor
PORT=3000
NODE_ENV=development

# Base de Datos (MySQL) - RELLENAR ESTOS DATOS
DB_HOST=127.0.0.1
DB_USER=root
DB_PASSWORD=your_db_password
DB_NAME=sas_c4_db
DB_PORT=3306

# Seguridad
JWT_SECRET=your_jwt_secret

# Integración C5
C5_API_URL=http://localhost:4000/api/recepcion/c4

# Integración AWS SQS
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-2
AWS_SQS_QUEUE_URL=https://sqs.us-east-2.amazonaws.com/your-account-id/sas-emergencias-queue
AWS_SQS_RESPONSE_QUEUE_URL=https://sqs.us-east-2.amazonaws.com/your-account-id/sas-respuestas-queue
